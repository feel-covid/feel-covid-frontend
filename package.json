{
  "name": "devtalk-web-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@reach/router": "^1.2.1",
    "date-fns": "^2.9.0",
    "i18next": "^19.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "immer": "^5.3.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.8.0",
    "react-i18next": "^11.3.1",
    "react-scripts": "3.3.1",
    "styled-components": "^5.0.0",
    "tailwindcss": "^1.2.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "build:css": "postcss src/tailwind.css -o src/tailwind.output.css --env production",
    "watch:css": "postcss src/tailwind.css -o src/tailwind.output.css -w",
    "start": "concurrently \"npm run watch:css\" \"react-scripts start\"",
    "build": "npm run build:css && set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "prettier-eslint src/**/*.{ts,tsx} --write",
    "preinstall": "node ./preInstallScript.js",
    "build:analyze": "cross-env ANALYZE=true craco build"
  },
  "engines": {
    "node": ">= 12.10.0",
    "yarn": ">= 1.21.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    ".{ts,tsx}": [
      "yarn lint"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.3",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-info": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "autoprefixer": "^9.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "postcss-cli": "^7.1.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "serve": "^11.3.0",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
