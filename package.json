{
	"name": "feel-covid-frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@fullhuman/postcss-purgecss": "^2.1.0",
		"@sentry/browser": "^5.15.4",
		"date-fns": "^2.11.1",
		"i18next": "^19.3.3",
		"i18next-xhr-backend": "^3.2.2",
		"lodash": "^4.17.15",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-i18next": "^11.3.4",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.4.0",
		"recharts": "1.8.5",
		"styled-components": "^5.0.1",
		"tailwindcss": "^1.2.0",
		"typescript": "~3.8.3"
	},
	"scripts": {
		"build:css": "postcss src/tailwind.css -o src/tailwind.output.css --env production",
		"watch:css": "postcss src/tailwind.css -o src/tailwind.output.css -w",
		"start": "concurrently \"npm run watch:css\" \"react-scripts start\"",
		"build": "yarn test && cross-env GENERATE_SOURCEMAP=false react-scripts build",
		"test": "react-scripts test --coverage",
		"eject": "react-scripts eject",
		"start:storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"lint": "eslint src/**/*.{ts,tsx}",
		"lint:fix": "prettier-eslint src/**/*.{ts,tsx} --write",
		"preinstall": "node ./preInstallScript.js",
		"build:analyze": "cross-env ANALYZE=true craco build"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint && yarn test"
		}
	},
	"lint-staged": {
		".{ts,tsx}": [
			"yarn lint"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			"last 1 chrome version"
		],
		"development": [
			"last 1 chrome version"
		]
	},
	"devDependencies": {
		"@craco/craco": "^5.6.3",
		"@storybook/addon-actions": "^5.3.17",
		"@storybook/addon-info": "^5.3.17",
		"@storybook/addon-links": "^5.3.17",
		"@storybook/addons": "^5.3.17",
		"@storybook/preset-create-react-app": "^2.1.0",
		"@storybook/react": "^5.3.17",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^10.0.3",
		"@testing-library/user-event": "^10.0.0",
		"@types/jest": "^25.1.4",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.11.1",
		"@types/react": "^16.9.25",
		"@types/react-dom": "^16.9.5",
		"@types/react-router": "^5.1.4",
		"@types/react-router-dom": "^5.1.3",
		"@types/recharts": "^1.8.9",
		"@types/styled-components": "^5.0.1",
		"@typescript-eslint/eslint-plugin": "^2.24.0",
		"@typescript-eslint/parser": "^2.24.0",
		"autoprefixer": "^9.7.4",
		"awesome-typescript-loader": "^5.2.1",
		"concurrently": "^5.1.0",
		"cross-env": "^7.0.2",
		"eslint": "6.8.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-node": "^4.0.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"husky": "^4.2.3",
		"jest-styled-components": "^7.0.2",
		"postcss-cli": "^7.1.0",
		"prettier": "^1.19.1",
		"prettier-eslint": "^9.0.1",
		"prettier-eslint-cli": "^5.0.0",
		"react-docgen-typescript-loader": "^3.7.1",
		"serve": "^11.3.0",
		"webpack-bundle-analyzer": "^3.6.1"
	}
}
